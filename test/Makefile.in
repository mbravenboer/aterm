# In maintainer mode we assume that bootstrapping utilities such as
# toolbus -gentifs, tifstoc, tifstojava, apigen and asfc are available
# on the path. If not, we will use touch to update the timestamp of
# the needed file if it exists, and fail if it does not exist.
# Alas, the 'which' command is not portable, so we have to hack it here:
@MAINTAINER_MODE_TRUE@check_available = \
@MAINTAINER_MODE_TRUE@  @found="";  \
@MAINTAINER_MODE_TRUE@  for p in `echo ${PATH} | tr ':' ' '`; do \
@MAINTAINER_MODE_TRUE@    if [ -f $$p/$1 ]; then \
@MAINTAINER_MODE_TRUE@      found=$$p; \
@MAINTAINER_MODE_TRUE@    fi; \
@MAINTAINER_MODE_TRUE@  done; \
@MAINTAINER_MODE_TRUE@  if [ -f $${found}/$1 ]; then \
@MAINTAINER_MODE_TRUE@    echo "$2"; $2; \
@MAINTAINER_MODE_TRUE@  else \
@MAINTAINER_MODE_TRUE@    echo "missing $1 to generate $@";  \
@MAINTAINER_MODE_TRUE@    if [ -f $@ ]; then \
@MAINTAINER_MODE_TRUE@      echo "touch $@"; touch $@; \
@MAINTAINER_MODE_TRUE@    else \
@MAINTAINER_MODE_TRUE@      echo "missing $@!";  \
@MAINTAINER_MODE_TRUE@      false; \
@MAINTAINER_MODE_TRUE@    fi;  \
@MAINTAINER_MODE_TRUE@  fi

